---- contiene los clientes de davivienda marcados por unidad de cumplimiento 
select KEY_ID, COD_TIPO_IDENTIFICACION,DESCRIPCION_IDENTIFICACION, NOMBRES_APELLIDO_CLIENTE, COD_PAIS_NACIMIENTO, DESCRIPCION_PAIS_NACIMIENTO, COD_CIUDAD_NACIMIENTO,DESCRIPCION_PAIS_NACIMIENTO, COD_CIUDAD_NACIMIENTO, DESCRIPCION_CIUDAD_NACIMIENTO, FECHA_NACIMIENTO, FECHA_EXPEDICION_IDENTIFICACION, DESCRIPCION_CIUDAD_EXPEDICION, DESCRIPCION_CIUDAD_EXPEDICION, DESCRIPCION_ACTIVIDAD_ECONOMICA, MONTO_TOTAL_ACTIVOS, MONTO_TOTAL_PASIVOS, NACIONALIDAD_CLIENTE, FLG_CLIENTE_LISTAS_RESTRICTIVAS, COD_AREA_RESTRICCION, DESCRIPCION_AREA_RESTRICCION, FLG_FUNCIONARIO_PUBLICO,FLG_PERSONA_POLITICA_EXPUESTA
from sb-ecosistemaanalitico-lago.davivienda.t_pn_datos_basicos
where COD_AREA_RESTRICCION is not null
