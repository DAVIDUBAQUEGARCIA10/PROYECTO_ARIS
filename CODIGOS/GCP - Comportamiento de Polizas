SELECT 
    CODIGO_COMPANIA,
    CODIGO_RAMO_EMISION,
    NOMBRE_RAMO_EMISION,
    CODIGO_RAMO_CONTABLE,
    NOMBRE_RAMO_CONTABLE,
    CODIGO_PRODUCTO,
    NOMBRE_PRODUCTO,
    CODIGO_SUBPRODUCTO,
    NOMBRE_SUBPRODUCTO,
    NOMBRE_CANAL,
    NUMERO_SECUENCIA_POLIZA,
    NUMERO_POLIZA_ANTERIOR,
    NUMERO_POLIZA_COASEGURO,
    NUMERO_POLIZA,
    NUMERO_POLIZA_SIGUIENTE,
    TIPO_DOCUMENTO_TOMADOR,
    KEY_ID_TOMADOR,
    TIPO_DOCUMENTO_ASEGURADO,
    KEY_ID_ASEGURADO,
    CAST(FECHA_EMISION_ENDOSO AS DATE) AS FECHA_EMISION_ENDOSO,
    CAST(FECHA_INICIO_ENDOSO AS DATE) AS FECHA_INICIO_ENDOSO,
    CAST(FECHA_FIN_ENDOSO AS DATE) AS FECHA_FIN_ENDOSO,
    CAST(FECHA_INICIO_POLIZA AS DATE) AS FECHA_INICIO_POLIZA,
    CAST(FECHA_FIN_POLIZA AS DATE) AS FECHA_FIN_POLIZA,
    PERIODO_FACTURACION,
    VALOR_ASEGURADO,
    PRIMA_EMITIDA_CEDIDA,
    PRIMA_EMITIDA_RETENIDA,
    VALOR_ASEGURADO_RETENIDO,
    PRIMA_EMITIDA_FACULTATIVO,
    VALOR_ASEGURADO_FACULTATIVO,
    PRIMA_EMITIDA_CEDIDA_ACUMU,
    VALOR_ASEGURADO_CEDIDO_ACUMU,
    COD_MOTIVO_CANCELACION_POLIZA,
    DES_MOTIVO_CANCELACION_POLIZA,
    CAST(FECHA_PROCESO AS DATE) AS FECHA_PROCESO,
    NIT_DAVIVIENDA,
    CAST(FECHA_ORIGEN AS DATE) AS FECHA_ORIGEN,
    OPCION_POLIZA
FROM 
    sb-ecosistemaanalitico-lago.seguros_bolivar.t_polizas_riesgos_endosos
where FECHA_PROCESO >'2024-06-01' and FECHA_ORIGEN >'2024-06-01'


