SELECT DISTINCT
    CODIGO_COMPANIA,
    CODIGO_RAMO_EMISION,
    NOMBRE_RAMO_EMISION,
    CODIGO_PRODUCTO,
    NOMBRE_PRODUCTO,
    NUMERO_POLIZA,
    TIPO_DOCUMENTO_TOMADOR,
    KEY_ID_TOMADOR,    
    VALOR_ASEGURADO,
    COD_MOTIVO_CANCELACION_POLIZA,
    DES_MOTIVO_CANCELACION_POLIZA,
    CAST(FECHA_EMISION_ENDOSO AS DATE) AS FECHA_EMISION_ENDOSO,
    CAST(FECHA_INICIO_ENDOSO AS DATE) AS FECHA_INICIO_ENDOSO,
    CAST(FECHA_FIN_ENDOSO AS DATE) AS FECHA_FIN_ENDOSO,
    CAST(FECHA_INICIO_POLIZA AS DATE) AS FECHA_INICIO_POLIZA,
    CAST(FECHA_FIN_POLIZA AS DATE) AS FECHA_FIN_POLIZA,
    TIPO_ENDOSO,
    DESCRIPCION_ENDOSO
FROM 
    sb-ecosistemaanalitico-lago.seguros_bolivar.t_polizas_riesgos_endosos
WHERE 
    CAST(FECHA_FIN_POLIZA AS DATE) > '2024-11-01' 
    AND CAST(FECHA_FIN_ENDOSO AS DATE) > '2024-11-01' 
    AND CAST(FECHA_EMISION_ENDOSO AS DATE) > '2024-01-01' 
    AND CAST(FECHA_INICIO_ENDOSO AS DATE) > '2024-01-01' 
    AND (DESCRIPCION_ENDOSO LIKE '%AUMENTO%' OR DESCRIPCION_ENDOSO LIKE '%INCREMENTO%') AND DESCRIPCION_ENDOSO LIKE '%ASEGURADO%'
