SELECT
    TIPO_DOCUMENTO,            -- Tipo de documento de identificación del cliente
    KEY_ID,                    -- Número de identificación del cliente o tercero
    NOMBRES,                   -- Nombres del cliente
    OBSERVACIONES,             -- Observaciones al registro si aplican
    CODIGO_LISTA,              -- Código que identifica una lista específica por la cual el cliente aparece en la lista restrictiva
    NOMBRE_LISTA,              -- Nombre de la lista restrictiva en la que se encuentra el cliente
    FECHA_INICIO,              -- Fecha de inicio del primer producto del cliente
    FECHA_FIN                  -- Fecha de finalización del registro o del producto del cliente
FROM
    `sb-ecosistemaanalitico-lago.seguros_bolivar.t_lista_restrictivas`;
