   
    SELECT DISTINCT 
    a.COD_CIA,
    a.TDOC_TERCERO,
    a.NRO_DOCUMTO,
    a.NUM_POL1,
    a.COD_RAMO,
    a.FECHA_EMI,
    a.FECHA_EMI_END,
    a.FECHA_VENC_POL,
    a.COD_PROD,
    b.*
FROM A2000030 a
JOIN A2001000 b 
    ON  a.NUM_SECU_POL = b.NUM_SECU_POL
WHERE a.FECHA_VENC_POL >= '10/08/25'
and MCA_ANU_POL != 'S'
and NRO_DOCUMTO = 40369772 

