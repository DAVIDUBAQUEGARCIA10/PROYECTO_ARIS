FROM juridicos j
LEFT JOIN acteco_tercero a ON j.SECUENCIA = a.JUR_SECUENCIA
LEFT JOIN actividades_economicas ae ON a.ACTECO_SECUENCIA = ae.SECUENCIA
--WHERE NAZ_CODIGO IN (2,3) 
)
SELECT *
FROM base
WHERE 
    (EXCEPTUADO_SARLAFT_SISTEM = 'S' AND REAL_EXCEPTUADO = 'No')
    OR
    (EXCEPTUADO_SARLAFT_SISTEM = 'N' AND REAL_EXCEPTUADO = 'Sí');
