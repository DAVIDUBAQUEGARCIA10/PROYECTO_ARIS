SELECT DISTINCT *
FROM ROLES_TERCERO rt
JOIN JURIDICOS n ON rt.secuencia = n.secuencia
JOIN ROLES r ON rt.ROL_CODIGO = r.CODIGO
WHERE r.CODIGO IN (84, 92, 5, 54, 59, 67, 78, 79, 93, 80, 81, 21, 41, 32, 62, 17, 65);


SELECT DISTINCT *
FROM ROLES_TERCERO rt
JOIN NATURALES n ON rt.secuencia = n.secuencia
JOIN ROLES r ON rt.ROL_CODIGO = r.CODIGO
WHERE r.CODIGO IN (84, 92, 5, 54, 59, 67, 78, 79, 93, 80, 81, 21, 41, 32, 62, 17, 65);
