SELECT
    TIPO_DOCUMENTO,
    KEY_ID_BENEFICIARIO,
    TITULO,
    'CAP_0304' AS ALERTA,    
    1 AS COMPANIA,
    FECHA_ALERTA
FROM
    `sb-sandbox-usuarios.sandbox_cumplimiento.CAP_0304_VISOR_REPO`

UNION ALL

SELECT
    TIPO_DOCUMENTO,
    KEY_ID,
    NUMERO_TITULO,
    'CAP_0069' AS ALERTA,
    1 AS COMPANIA,
    FECHA_ALERTA
FROM
    `sb-sandbox-usuarios.sandbox_cumplimiento.CAP_0069_VISOR_TRANS`

UNION ALL

SELECT
    TIPO_DOCUMENTO,
    KEY_ID_TOMADOR,
    NUMERO_TITULO,
    'CAP_0009' AS ALERTA,
    1 AS COMPANIA,
    FECHA_ALERTA
FROM
    `sb-sandbox-usuarios.sandbox_cumplimiento.CAP_0009_VISOR_TRANS`
