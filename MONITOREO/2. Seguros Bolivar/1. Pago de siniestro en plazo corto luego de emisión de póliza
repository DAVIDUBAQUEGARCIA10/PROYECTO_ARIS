SELECT 
    CODIGO_COMPANIA, 
    CODIGO_RAMO_CONTABLE, 
    NOMBRE_RAMO_CONTABLE,
    CODIGO_PRODUCTO,
    NOMBRE_PRODUCTO,
    NUMERO_SECUENCIA_POLIZA,
    NUMERO_POLIZA,
    TIPO_DOCUMENTO_TOMADOR,
    KEY_ID_TOMADOR,
    FECHA_SINIESTRO,
    FECHA_SUSCRIPCION,
    ESTADO_SINIESTRO,
    FECHA_ORDEN_PAGO
FROM 
    sb-ecosistemaanalitico-lago.seguros_bolivar.t_siniestros
WHERE 
    FECHA_SUSCRIPCION >= '2024-01-01'
    AND DATE_DIFF(DATE(FECHA_SINIESTRO), DATE(FECHA_SUSCRIPCION), DAY) <= 90
    AND RIGHT(CAST(NUMERO_POLIZA AS STRING), 2) = '01';  -- Filtrar por pÃ³lizas que terminan en '01'
    AND CODIGO_COMPANIA = 2

