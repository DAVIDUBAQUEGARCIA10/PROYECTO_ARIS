-- Insertar solo los registros nuevos que no existan en la tabla COM_0068_VISOR_REPO
INSERT INTO sb-sandbox-usuarios.sandbox_cumplimiento.COM_0068_VISOR_REPO
(
    CODIGO_COMPANIA,
    CODIGO_RAMO_EMISION,
    NOMBRE_RAMO_EMISION,
    CODIGO_PRODUCTO,
    NOMBRE_PRODUCTO,
    NUMERO_POLIZA,
